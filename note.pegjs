{
    function noteLetterToInt(c) {
        if (c === 'C') { 
            return 0;
        }
        if (c === 'D') { 
            return 2;
        }
        if (c === 'E') { 
            return 4;
        }
        if (c === 'F') { 
            return 5;
        }
        if (c === 'G') { 
            return 7;
        }
        if (c === 'A') { 
            return 9;
        }
        if (c === 'B') { 
            return 11;
        }
    }
}
